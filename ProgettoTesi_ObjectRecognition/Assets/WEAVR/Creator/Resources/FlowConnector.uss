}

.flowPort.port.output.Standby {
}

.flowPort.port.output.Ready {
}

.flowPort.port.output.Running {
    --port-color: #e5e900;
}

.flowPort.port.output.Finished {
    --port-color: #16b200;
}

.flowPort.port.output.Faulted {
    --port-color: #f10000;
}

.flowPort.port.output.ForceStopped {
}

