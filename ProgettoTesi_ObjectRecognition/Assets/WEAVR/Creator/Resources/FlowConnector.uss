/*SIMPLE ACTION STYLES*/

.flowEdge .edge-control {
    justify-content: center;
    align-items: center;
}

/*--------------------------- UNREACHEABLE PART ----------------------------------*/

.unreacheable .flowPort.port {
    --port-color: #888888;
}

    .unreacheable .flowPort.port.output {
        --port-color: #888888;
    }

/*--------------------------- FAST FORWARD PART ----------------------------------*/

.edge.fast-forward-path {
    --edge-width: 7;
    --input-edge-color: #d79a01;
    --output-edge-color: #d79a01;
}

    .edge.fast-forward-path.high-priority {
        --edge-width: 7;
        --input-edge-color: #d75801;
        --output-edge-color: #d75801;
    }

.edge.high-priority {
    --edge-width: 7;
    --input-edge-color: #daa078;
    --output-edge-color: #daa078;
}

/*-------------------------- ACTION CONTAINER PART ------------------------------*/
.actions-container {
    background-image: resource("images/flyout_center_9slice");
    position: absolute;
    width: 280px;
    margin-top: 4px;
    padding: 26px 16px 16px;
    -unity-slice-top: 40;
    -unity-slice-bottom: 25;
    -unity-slice-left: 10;
    -unity-slice-right: 10;
}

    .actions-container .simpleActionView {
        --refresh-color: #009bfd;
        --progress-update-ms: 33;
    }

        .actions-container .simpleActionView #contents {
            background-color: rgba(10, 10, 10, 0.85);
            flex-direction: row;
            margin-top: 2px;
            padding-top: 0px;
            padding-bottom: 0px;
            border-width: 1px;
            /*border-top-width: 2px;*/
            border-color: #003041;
            border-radius: 4px;
            margin: 1px 0px;
        }

        .actions-container .simpleActionView.first {
            margin-top: -2px;
        }

        .actions-container .simpleActionView #description {
            flex-grow: 1;
            white-space: normal;
            padding: 2px;
            margin-right: 22px;
            flex-wrap: wrap;
            flex-direction: row;
            align-content: stretch;
            align-self: stretch;
            -unity-text-align: middle-left;
        }

        .actions-container .simpleActionView #colored-box {
            width: 2px;
            background-color: #4cff00;
        }

        .actions-container .simpleActionView #icon {
            width: 18px;
            height: 18px;
            align-self: center;
            padding: 2px;
        }

        .actions-container .simpleActionView #progress-bar {
            left: 0px;
            top: 0px;
            bottom: 0px;
            margin-right: 2px;
            position: absolute;
            background-color: rgba(0, 158, 48, 0.50);
        }

        .actions-container .simpleActionView.with-warning #contents {
            border-width: 1px;
            border-color: #f4af00;
        }

        .actions-container .simpleActionView.with-error #contents {
            border-width: 1px;
            border-color: #ff6464;
        }

        .actions-container .simpleActionView .icon-badge {
            position: absolute;
            right: -3px;
            top: 2px;
            bottom: 0px;
        }

            .actions-container .simpleActionView .icon-badge #tip {
                display: none;
            }

            .actions-container .simpleActionView .icon-badge #icon {
                position: absolute;
                left: 0px;
                top: 0px;
                bottom: 0px;
            }

        /*WITH STATES*/

        .actions-container .simpleActionView.Ready {
        }

        .actions-container .simpleActionView.Started {
        }

        .actions-container .simpleActionView.Running {
            background-color: #0066a7;
        }

        .actions-container .simpleActionView.Finished {
            background-color: #197c00;
        }

        .actions-container .simpleActionView.Faulted {
            background-color: #f10000;
        }

            .actions-container .simpleActionView.Faulted #progress-bar {
                background-color: #f10000;
            }

        .actions-container .simpleActionView.Paused {
        }

        .actions-container .simpleActionView.ForceStopped {
        }

        .actions-container .simpleActionView.Skipped {
        }

            .actions-container .simpleActionView.Skipped #description {
                color: rgba(232, 232, 232, 0.50);
            }

        .actions-container .simpleActionView.Breakpoint {
        }
