

.flowPort #connector #hole #cap {
    width: 0px;
    height: 0px;
}

.flowPort #connector.portHighlight #hole #cap {
    background-color: rgb(220, 220, 220);
}

.flowPort.port {
    padding-left: 0px;
    padding-right: 0px;
    margin-left: 12px;
    margin-right: 12px;
    width: 100px;
    height: 28px;
    --port-color: rgb(220, 220, 220);
    flex-direction: column-reverse;
}

    .flowPort.port.output {
        flex-direction: column;
    }


    .flowPort.port #connector {
        width: 12px;
        height: 12px;
        margin-left: 2px;
        margin-right: 2px;
    }

    .flowPort.port.mandatory #connector {
        width: 14px;
        height: 14px;
    }

    .flowPort.port #type {
        position: absolute;
        width: 42px;
        top: 1px;
        left: 58px;
        margin-left: 0px;
        margin-right: 0px;
    }


    .flowPort.port #connector #cap {
        width: 6px;
        height: 6px;
        border-radius: 0 4px 4px 0;
    }

.flowPort.connected #connector {
}

.flowEdge.edge {
    --edge-width: 4;
    border-color: rgba(146,146,146,255);
}

.flowEdge.mandatory {
    --edge-width: 6;
}

.flowEdge.edge.selected {
    border-color: rgba(240,240,240,255);
}

.flowEdge.edge:hover {
    --edge-width: 7;
}

.flowEdge.mandatory:hover {
    --edge-width: 8;
}

.flowPort.port.output {
    --port-color: rgb(160, 160, 160);
}

    .flowPort.port.output.mandatory {
        --port-color: rgb(220, 220, 220);
    }

    .flowPort.port.output.with-actions {
        --port-color: #00c0fe;
    }

    .flowPort.port.output.mandatory.with-actions {
        --port-color: #00c0fe;
    }

